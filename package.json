{
  "name": "dvsa-rsp-web-public",
  "version": "4.0.3",
  "description": "DVSA Public Payment Portal For RSP",
  "engines": {
    "node": "20.x"
  },
  "main": "handler.js",
  "scripts": {
    "bundle": "npm run clean && npm run build-prod",
    "build": "npm run build-server && npm run build-frontend",
    "build-server": "babel ./src/server --out-dir ./build/server --ignore ./src/server/public/ --copy-files --source-maps",
    "build-frontend": "webpack --config webpack.development.js",
    "build-prod": "npm run build-server && webpack --config webpack.production.js",
    "start-server": "npm run build-server && node build/server/app.local.js",
    "start-dev-server": "concurrently \"npm run build-frontend\" \"nodemon -e js,njk --watch src/server --exec npm run start-server\"",
    "clean": "node_modules/.bin/rimraf dist",
    "package": "node scripts/i18n-shim.js && npm run bundle",
    "test": "mocha --require @babel/register test/**/*.js",
    "test:watch": "mocha --require @babel/register test/**/*.js --watch",
    "test:coverage": "nyc npm run test",
    "prepush": "npm test && npm run lint && npm run check-security && npm run check-security-log",
    "lint": "./node_modules/.bin/eslint src/server src/lambdas",
    "lint:fix": "./node_modules/.bin/eslint --fix src/server src/lambdas",
    "check-security": "git secrets --scan",
    "check-security-log": "git log -p | scanrepo"
  },
  "author": "BJSS",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.18.10",
    "@babel/register": "^7.18.9",
    "@babel/runtime-corejs3": "^7.18.9",
    "@istanbuljs/nyc-config-babel": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-istanbul": "^6.1.1",
    "blissfuljs": "1.0.6",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "classlist": "^2.0.0",
    "classlist.js": "^1.1.20150312",
    "concurrently": "^7.3.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "current-git-branch": "^2.0.2",
    "element-closest": "^3.0.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-webpack-plugin": "^3.2.0",
    "govuk-frontend": "^5.12.0",
    "md5": "^2.3.0",
    "mocha": "^10.0.0",
    "nock": "^13.2.9",
    "nodemon": "^2.0.19",
    "nyc": "^15.1.0",
    "regenerator-runtime": "^0.13.9",
    "require-dir": "^1.2.0",
    "rimraf": "^3.0.2",
    "sass": "^1.93.2",
    "serverless": "^3.21.0",
    "serverless-offline": "^9.2.0",
    "sinon": "^14.0.0",
    "smooth-scroll": "^16.1.3",
    "store": "^2.0.12",
    "webpack": "^5.101.3",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^5.2.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@dvsa/cookie-manager": "^1.1.2",
    "archiver": "^7.0.0",
    "aws-sdk": "^2.1354.0",
    "aws-serverless-express": "^3.4.0",
    "aws4": "^1.11.0",
    "axios": "^1.12.0",
    "axios-retry": "^3.3.1",
    "body-parser": "^1.20.3",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.0.1",
    "errorhandler": "^1.5.1",
    "event-stream": "4.0.1",
    "express": "^4.19.2",
    "express-validator": "^7.3.0",
    "govuk_frontend_toolkit": "^9.0.1",
    "govuk_template_jinja": "^0.26.0",
    "helmet": "^5.1.1",
    "i18n-express": "^1.1.3",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.34",
    "nocache": "^3.0.4",
    "node-dir": "^0.1.17",
    "nunjucks": "^3.2.4",
    "resolve-path": "^1.4.0",
    "url-parse": "^1.5.10",
    "walk-sync": "^3.0.0"
  }
}
